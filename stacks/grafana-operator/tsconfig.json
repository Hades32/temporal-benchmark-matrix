{
    "compilerOptions": {
        "outDir": "bin",
        "target": "es2016",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": true,
        "sourceMap": true,
        "stripInternal": true,
        "experimentalDecorators": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true
    },
    "files": [
        "grafana/index.ts",
        "grafana/v1beta1/grafana.ts",
        "grafana/v1beta1/grafanaDashboard.ts",
        "grafana/v1beta1/grafanaDatasource.ts",
        "grafana/v1beta1/grafanaFolder.ts",
        "grafana/v1beta1/index.ts",
        "index.ts",
        "provider.ts",
        "types/index.ts",
        "types/input.ts",
        "types/output.ts",
        "utilities.ts"
    ]
}
